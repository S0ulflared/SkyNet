{
  "manifest_version": 3,
  "name": "skynet",
  "version": "0.0.1",
  "description": "A custom new tab page with a search bar and logo.",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_title": "skynet",
    "default_icon": {
      "16": "icon16.png",   
      "48": "icon48.png",   
      "128": "https://upload.wikimedia.org/wikipedia/commons/2/20/Skynet_icon_transparent_background.png"  
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}